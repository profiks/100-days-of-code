{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "indent": ["error","tab"],
        "linebreak-style": ["error", "unix"],
        "strict": 1,
        "quotes": ["error","single"],
        "semi": ["error","never"]
    }
}